{
    "name": "nguyenanhung/web-builder-helper",
    "type": "library",
    "description": "Web Builder Helper by Hung Nguyen",
    "keywords": [
        "php",
        "website",
        "news"
    ],
    "homepage": "https://github.com/nguyenanhung/web-builder-helper",
    "license": "GPL-3.0-only",
    "authors": [
        {
            "name": "Nguyen An Hung",
            "email": "dev@nguyenanhung.com",
            "homepage": "https://nguyenanhung.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.0",
        "ext-pdo": "*",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "nguyenanhung/filtered-helper": "^2.0 || ^1.0",
        "nguyenanhung/hashids-helper": "^2.0 || ^1.0",
        "nguyenanhung/slug-helper": "^2.0 || ^1.0",
        "nguyenanhung/tracking-tag-helper": "^2.0 || ^1.0",
        "nguyenanhung/markdown": "^2.0 || ^1.0",
        "nguyenanhung/web-builder-assets": "^1.0",
        "nesbot/carbon": "^2 || ^1"
    },
    "require-dev": {
        "nguyenanhung/my-cache": "^3.0 || ^2.0 || ^1.0",
        "nguyenanhung/seo": "^3.0 || ^2.0 || ^1.0",
        "nguyenanhung/image": "^3.0 || ^2.0 || ^1.0",
        "nguyenanhung/helpers": "^3.0 || ^2.0 || ^1.0"
    },
    "suggest": {
        "nguyenanhung/image": "Image Cache & Image processing",
        "nguyenanhung/seo": "SEO Support",
        "nguyenanhung/my-cache": "Cache Support",
        "nguyenanhung/helpers": "For Awesome Helpers and Library"
    },
    "autoload": {
        "psr-4": {
            "nguyenanhung\\WebBuilderHelper\\": "src/"
        },
        "files": [
            "helpers/helpers.php"

        ]
    },
    "config": {
        "allow-plugins": {
            "kylekatarnls/update-helper": true
        }
    }
}
